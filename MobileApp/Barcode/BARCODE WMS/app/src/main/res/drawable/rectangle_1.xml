<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape  android:shape="rectangle">
            <solid android:color="#DFDFDF" />
            <gradient
                android:type="sweep"
                android:angle="0"
                android:startColor="#D1D1D1"
                android:endColor="#CDCDCD" />
            <corners android:radius="15dp" />
        </shape>
    </item>
    <item android:left="3dp" android:bottom="3dp" android:right="4dp" android:top="3dp">
        <shape  android:shape="rectangle">
            <solid android:color="@color/white_two" />
            <stroke android:width="1dp" android:color="@color/white_two" />
            <corners android:radius="10dp" />
        </shape>
    </item>
</layer-list>